{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.9/MicrosoftTeams.schema.json",
  "manifestVersion": "1.9",
  "version": "1.0.0",
  "id": "[APPLICATION ID]",
  "packageName": "com.teams.ev.fdd",
  "developer": {
    "name": "fdd",
    "websiteUrl": "https://www.fdd.com",
    "privacyUrl": "https://www.fdd.com/privacy",
    "termsOfUseUrl": "https://www.fdd.com/termsofuse"
  },
  "icons": {
    "color": "bot-icon-192x192.png",
    "outline": "bot-icon-outline-32x32.png"
  },
  "name": {
    "short": "Training Auth",
    "full": "Training GSP"
  },
  "description": {
    "short": "Training Auth in Teams 2021-06",
    "full": "Demos Teams"
  },
  "accentColor": "#0EA20E",
  "staticTabs": [
    {
      "entityId": "tabsso",
      "name": "SSO",
      "contentUrl": "https://www.fdd.com/sso/sso.html",
      "websiteUrl": "https://www.fdd.com",
      "scopes": [
        "personal"
      ]
    },
    {
      "entityId": "tabsilent",
      "name": "Silent",
      "contentUrl": "https://www.fdd.com/silent/tabsilentauthenticationstart.html",
      "websiteUrl": "https://www.fdd.com",
      "scopes": [
        "personal"
      ]
    },
    {
      "entityId": "tabauthentication",
      "name": "Authentication",
      "contentUrl": "https://www.fdd.com/tab/tabauthenticate.html",
      "websiteUrl": "https://www.fdd.com",
      "scopes": [
        "personal"
      ]
    }
  ],
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "validDomains": [
    "*.ngrok.io"
  ],
  "webApplicationInfo": {
    "id": "[CLIENT ID]",
    "resource": "api://www.fdd.com/[CLIENT ID]"
  }
}